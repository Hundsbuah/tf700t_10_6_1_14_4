on property:wifi.module.type=4
    insmod /system/lib/modules/bcmdhd.ko

on property:wifi.module.type=5
    insmod /system/lib/modules/bcmdhd_34.ko

on property:wifi.module.type=6
    insmod /system/lib/modules/bcmdhd_34.ko

